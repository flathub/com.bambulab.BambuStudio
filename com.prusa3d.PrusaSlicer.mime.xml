<?xml version="1.0" encoding="utf-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/vnd.ms-3mfdocument">
        <comment>3D Manufacturing File</comment>
        <sub-class-of type="application/zip"/>
        <glob pattern="*.3mf"/>
        <icon name="com.prusa3d.PrusaSlicer.application-vnd-ms-3mfdocument"/>
        <generic-icon name="com.prusa3d.PrusaSlicer.application-vnd-ms-3mfdocument"/>
    </mime-type>
    <mime-type type="text/x.gcode">
        <comment>G-CODE</comment>
        <glob pattern="*.gcode"/>
        <icon name="com.prusa3d.PrusaSlicer.text-x-gcode"/>
        <generic-icon name="com.prusa3d.PrusaSlicer.text-x-gcode"/>
    </mime-type>
</mime-info>